(3639, 375, 'inquiry_email_form', '<form role=\"form\" id=\"question_answer_form\">\r\n<div class=\"row\">\r\n<div class=\"col-md-12\">\r\n\r\n<div class=\"form-group col-md-6 padding-l-0\">\r\n<label>Division</label>\r\n<select id=\"division\" class=\"form-control\" name=\"division\">\r\n<option value=\"\">&#8211;</option>\r\n<option value=\"1SD\">1SD</option>\r\n<option value=\"2SD\">2SD</option>\r\n<option value=\"3SD\">3SD</option>\r\n<option value=\"CPD\">CPD</option>\r\n</select>\r\n</div>\r\n\r\n<div class=\"form-group col-md-6 padding-r-0\">\r\n<label>Project Name</label>\r\n<input id=\"project_name\" class=\"form-control\" name=\"project_name\" type=\"text\" placeholder=\"Project Name\" />\r\n</div>\r\n\r\n<div class=\"form-group margin-top-10 col-md-6 padding-l-0\">\r\n<label>Requester</label>\r\n<input id=\"requester\" class=\"form-control\" name=\"requester\" type=\"text\" placeholder=\"Requester\" />\r\n</div>\r\n\r\n<div class=\"form-group margin-top-10 col-md-6 padding-r-0\">\r\n<label>Emails</label>\r\n<i class=\"fa fa-question-circle\" title=\"&lt;b&gt;*Can add multiple emails&lt;br&gt;*Press TAB, &quot;,&quot; or ENTER to add another email&lt;/b&gt;\" data-toggle=\"tooltip\" data-html=\"true\" data-placement=\"top\"></i>\r\n<input id=\"emails\" class=\"form-control\" name=\"emails\" type=\"text\" placeholder=\"Emails\" data-role=\"tagsinput\" />\r\n</div>\r\n\r\n<div class=\"form-group margin-top-10 col-md-6 padding-l-0\">\r\n<label>Target OSS</label>\r\n<input id=\"target_oss\" class=\"form-control\" name=\"target_oss\" type=\"text\" placeholder=\"Target OSS\" />\r\n</div>\r\n\r\n<div class=\"form-group margin-top-10 col-md-6 padding-r-0\">\r\n<label>OSS Version</label>\r\n<input id=\"oss_version\" class=\"form-control\" name=\"oss_version\" type=\"text\" placeholder=\"OSS Version\" />\r\n</div>\r\n\r\n<div class=\"form-group margin-top-10 col-md-6 padding-l-0\">\r\n<label>Due Date</label>\r\n<input id=\"due_date\" class=\"form-control\" name=\"du