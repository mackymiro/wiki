(3447, 371, 'bug_patch_subscribe_form', '<!-- SUBSCRIPTION SUCCESSFUL OR ERROR MESSAGES -->\r\n                  <div class=\"hide subscription-success\"></div>\r\n                  <div class=\"hide subscription-error\"></div>\r\n                    <form role=\"form\" action=\"subscribe.php\" id=\"subscribe_form\" class=\"subscribe_form\">\r\n                        <div class=\"row\">\r\n                            <div class=\"col-md-12\">\r\n                              <div class=\"form-group col-md-6 padding-l-0\">\r\n                                <label>Name</label>\r\n                                <input type=\"text\" required class=\"validateFields form-control\" name=\"name\" id=\"name\" placeholder=\"Name\">\r\n                              </div>\r\n\r\n                              <div class=\"form-group col-md-6 padding-r-0\">\r\n                                <label>Email</label>\r\n                                <input type=\"text\" required class=\"validateFields form-control\" name=\"email\" id=\"email\" placeholder=\"Email\">\r\n                              </div>\r\n\r\n                              <div class=\"form-group margin-top-10 col-md-12 padding-l-0\">\r\n                                <label>Duration</label>\r\n															</div>\r\n                              <div class=\"form-group margin-top-10 col-md-6 padding-l-0\">\r\n                                <input type=\"text\" required class=\"validateFields duration form-control\" id=\"start_date\" name=\"start_date\" placeholder=\"mm/dd/yyyy\">\r\n                              </div>\r\n                              <div class=\"form-group margin-top-10 col-md-6 padding-l-0\">\r\n                                <input type=\"text\" required class=\"validateFields duration form-control\" id=\"end_date\" name=\"end_date\" placeholder=\"mm/dd/yyyy\">\r\n                              </div>\r\n\r\n                              <div class=\"form-group margin-top-10 col-md-6 padding-l-0\">\r\n                                <label>OSS</label>\r\n                                <select required class=\"validateFields form-control\" id=\"oss\" name=\"oss\">\r\n                                  <option style=\"color: black;\" value=\'\'>&#8211;</option>\r\n                                  <option style=\"color: black;\" value=\'Zabbix\'>Zabbix</option>\r\n                                 </select>\r\n                              </div>\r\n\r\n                              <div class=\"form-group margin-top-10 col-md-12 padding-l-0\">\r\n                                <button type=\"button\" class=\"btn btn-default subs-btn\" id=\"form-subscribe\">SUBSCRIBE</button>\r\n                              </div>\r\n                            </div>\r\n                        </div>\r\n                    </form>\r\n                    <!-- END FORM -->'),
