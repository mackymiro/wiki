(2667, 236, 'faq_problems_0_faq_problem_0_faq_problem_text', '<p>ndb node(ndbd) could not start</p>\r\n'),
(2668, 236, '_faq_problems_0_faq_problem_0_faq_problem_text', 'field_595b4da88dfe7'),
(2669, 236, 'faq_problems_0_faq_problem_1_faq_problem_text', '<p>ndbd could not complete startup since it could not complete its transaction with other ndb nodes due to firewall.</p>\r\n'),
(2670, 236, '_faq_problems_0_faq_problem_1_faq_problem_text', 'field_595b4da88dfe7'),
