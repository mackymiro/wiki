(2697, 237, 'faq_problems_0_faq_problem_0_faq_problem_text', '<p>Could not execute ndb_show_tables cluster commands.</p>\r\n'),
(2698, 237, '_faq_problems_0_faq_problem_0_faq_problem_text', 'field_595b4da88dfe7'),
(2699, 237, 'faq_problems_0_faq_problem_1_faq_problem_text', '<p>Could not display ndb tables in the cluster due to no free node id for ndb cluster command execution. This is also applicable to other ndb cluster commands like ndb_restore, ndb_desc.</p>\r\n'),
(2700, 237, '_faq_problems_0_faq_problem_1_faq_problem_text', 'field_595b4da88dfe7'),
(2701, 237, 'faq_problems_0_faq_problem', '2'),
